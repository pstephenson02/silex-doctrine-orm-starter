{
    "name": "pstephenson02/silex-doctrine-orm-starter",
    "description": "An opinionated starter project for Silex and Doctrine ORM",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^7.0",
        "silex/silex": "~2.0",
        "dflydev/doctrine-orm-service-provider": "^2.0",
        "sorien/silex-dbal-profiler": "^2.0",
        "symfony/validator": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "doctrine/data-fixtures": "^1.2",
        "symfony/browser-kit": "^3.2"
    },
    "autoload": {
        "psr-0": {
            "App": "src/",
            "tests\\integration\\App": "tests/integration/"
        }
    }
}
